datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}



generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}


model Product {
    id          Int       @id @default(autoincrement())
    name        String
    description String
    price       Decimal  @db.Decimal()
    image       String
}